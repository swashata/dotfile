{
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "editor.fontFamily": "Dank Mono",
  "editor.fontSize": 16,
  "editor.fontLigatures": false,
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "eslint.packageManager": "yarn",
  "php.suggest.basic": false,
  "debug.allowBreakpointsEverywhere": true,
  "editor.insertSpaces": false,
  "editor.letterSpacing": 0.5,
  "editor.renderWhitespace": "selection",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "plaintext": "jade",
    "vue-html": "html"
  },
  "window.title": "${activeEditorMedium}${separator}${rootName}${seperator}  ${dirty}",
  "eslint.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "workbench.statusBar.feedback.visible": false,
  "editor.rulers": [80],
  "explorer.confirmDragAndDrop": false,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "json.schemas": [
    {
      "fileMatch": ["cypress.json"],
      "url": "https://on.cypress.io/cypress.schema.json"
    },
    {
      "fileMatch": [".prettierrc.json"],
      "url": "http://json.schemastore.org/prettierrc"
    }
  ],
  "emmet.triggerExpansionOnTab": false,
  "javascript.validate.enable": false,
  "licenser.license": "GPLv3",
  "licenser.author": "Swashata Ghosh <swashata@wpquark.com>",
  "editor.tabSize": 2,
  "typescript.tsserver.log": "verbose",
  "workbench.list.horizontalScrolling": true,
  "php-docblocker.author": {
    "name": "Swashata Ghosh",
    "email": "swashata@wpquark.com"
  },
  "explorer.autoReveal": false,
  "files.associations": {
    "*.xml.dist": "xml",
    "*.json": "jsonc",
    "*.js": "javascriptreact"
  },
  "path-autocomplete.transformations": [
    {
      "type": "replace",
      "parameters": ["^_", ""],
      "when": {
        "fileName": "\\.scss$"
      }
    }
  ],
  "files.autoSave": "off",
  "editor.acceptSuggestionOnEnter": "off",
  "window.closeWhenEmpty": false,
  "editor.detectIndentation": true,
  "editor.showFoldingControls": "always",
  "editor.matchBrackets": true,
  "editor.trimAutoWhitespace": true,
  "editor.wordSeparators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~?",
  "editor.colorDecorators": true,
  "editor.dragAndDrop": false,
  "editor.find.autoFindInSelection": true,
  "editor.emptySelectionClipboard": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "none",
    "editor.detectIndentation": true
  },
  "window.zoomLevel": 0,
  "sync.gist": "81deae83240d1662212f8ffe50b2a44e",
  "sync.quietSync": false,
  "sync.askGistName": false,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.forceDownload": false,
  "npm.enableScriptExplorer": false,
  "git.autofetch": true,
  "editor.minimap.enabled": false,
  "files.defaultLanguage": "markdown",
  "intelephense.files.maxSize": 3500000,
  "intelephense.format.enable": false,
  "workbench.editor.labelFormat": "short",
  "extensions.ignoreRecommendations": true,
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.formatOnSave": true
  },
  "breadcrumbs.enabled": true,
  "editor.hover.delay": 500,
  "editor.parameterHints.enabled": true,
  "editor.tabCompletion": "on",
  "editor.quickSuggestionsDelay": 100,
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "spellright.language": ["en"],
  "spellright.notificationClass": "warning",
  "spellright.spellContext": "body comments",
  "php.validate.enable": false,
  "emmet.excludeLanguages": ["markdown", "php"],
  "explorer.confirmDelete": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    "semanticHighlighting": true,
    "[βui - One Dark]": {
      "semanticHighlighting": true
    },
    // customization for one dark pro
    "[One Dark Pro Vivid]": {
      "textMateRules": [
        {
          "name": "JS/TS Imports",
          "settings": {
            "foreground": "#61afefff",
            "fontStyle": ""
          },
          "scope": [
            "meta.import variable.other.readwrite.alias",
            "entity.name.type"
          ]
        },
        {
          "name": "JS/TS import from",
          "settings": {
            "fontStyle": "italic"
          },
          "scope": ["meta.import keyword.control.from"]
        },
        {
          "name": "JS/TS italics",
          "settings": {
            "fontStyle": "italic"
          },
          "scope": [
            "meta.var.expr storage.type",
            "keyword.control.flow",
            "keyword.operation.graphql"
          ]
        },
        {
          "name": "Function parameter",
          "settings": {
            "foreground": "#abb2bfff",
            "fontStyle": ""
          },
          "scope": ["variable.parameter"]
        }
      ]
    }
  },
  // customization for night owl
  "workbench.colorCustomizations": {
    "[Night Owl]": {
      "editorCursor.foreground": "#ff7b00",
      "editorGroup.border": "#344552",
      "sideBar.background": "#01121f",
      "sideBar.border": "#1a2d3d",
      "activityBar.background": "#01121f",
      "activityBar.border": "#1a2d3d",
      "tab.activeBorder": "#ff7b00",
      "tab.unfocusedActiveBorder": "#e66f00",
      "editorRuler.foreground": "#022a4b",
      "editorIndentGuide.background": "#022038",
      "editorIndentGuide.activeBackground": "#03467c"
    }
  },
  "gitlens.advanced.messages": {
    "suppressCommitNotFoundWarning": true
  },
  "licenser.disableAutoHeaderInsertion": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "experimentalWebgl",
  "eslint.run": "onSave",
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "underline",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.wordBasedSuggestions": false,
  "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
  "workbench.iconTheme": "city-lights-icons-vsc",
  "customizeUI.activityBar": "regular",
  "workbench.useExperimentalGridLayout": true,
  "customizeUI.font.monospace": "Dank Mono",
  "customizeUI.font.regular": "Menlo",
  "customizeUI.titleBar": "inline",
  "customizeUI.fontSizeMap": {
    "monospace": "12px",
    "tab-title": "12px"
  },
  "window.titleBarStyle": "native",
  "php-docblocker.useShortNames": true,
  "php-docblocker.returnGap": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "gitlens.advanced.telemetry.enabled": false,
  "liveshare.featureSet": "stable",
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "[php]": {
    "editor.defaultFormatter": "wongjn.php-sniffer"
  },
  "whiteviz.enabled": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.activityBar.visible": true,
  "typescript.reportStyleChecksAsWarnings": false,
  "todo-tree.highlights.defaultHighlight": {
    "icon": "issue-opened",
    "type": "tag",
    "foreground": "white",
    "background": "#80FF00",
    "opacity": 50,
    "iconColour": "#80FF00"
  },
  "errorLens.delay": 800,
  "intelephense.diagnostics.typeErrors": false,
  "editor.snippetSuggestions": "bottom",
  "path-intellisense.autoSlashAfterDirectory": true,
  "editor.lineHeight": 26,
  "todo-tree.tree.showScanModeButton": false,
  "workbench.colorTheme": "βui - One Dark",
  "workbench.fontAliasing": "antialiased",
  "oneDarkPro.italic": false
}
